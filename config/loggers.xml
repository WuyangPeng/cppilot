<?xml version="1.0" encoding="utf-8"?>
<loggers>
    <global_level>trace</global_level>
    <console_level>trace</console_level>
    <logger name="unregistered_logger">
        <level>trace</level>
        <console_enabled>true</console_enabled>
        <channel_name>unregistered</channel_name>
        <log_file_name>unregistered</log_file_name>
        <rotation_size>50</rotation_size>
    </logger>
    <logger name="common_logger">
        <level>trace</level>
        <console_enabled>true</console_enabled>
        <channel_name>common</channel_name>
        <log_file_name>common</log_file_name>
        <rotation_size>50</rotation_size>
    </logger>
    <logger name="config_logger">
        <level>trace</level>
        <console_enabled>true</console_enabled>
        <channel_name>config</channel_name>
        <log_file_name>config</log_file_name>
        <rotation_size>50</rotation_size>
    </logger>
</loggers>